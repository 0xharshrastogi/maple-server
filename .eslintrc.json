{
  "env": {
    "es2021": true
  },
  "extends": ["airbnb-base", "prettier"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "prettier/prettier": "error",
    "import/no-import-module-exports": "off",
    "no-underscore-dangle": "off"
  },
  "plugins": ["prettier"]
}
